{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "rootDir": "src",
        "outDir": "scripts",
        "removeComments": true,
        "pretty": true,
        "strict": true,
        "noImplicitAny": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noImplicitThis": true
    }
}